{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "laravel/sanctum": "^4.0",
        "laravel/octane": "^2.3",
        "laravel/ui": "^4.5",
        "guzzlehttp/guzzle": "^7.7",
        "predis/predis": "^2.2",
        "doctrine/dbal": "^3.8",
        "psr/http-message": "^1.1",
        "php-open-source-saver/jwt-auth": "^2.3",
        "laravel-doctrine/orm": "^2.1",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-backup": "^8.8",
        "spatie/flysystem-dropbox": "^3.0",
        "masbug/flysystem-google-drive-ext": "^2.3",
        "league/flysystem": "^3.28",
        "league/flysystem-aws-s3-v3": "^3.28",
        "google/apiclient": "^2.16",
        "webpatser/laravel-uuid": "^4.0",
        "unisharp/laravel-filemanager": "^2.9",
        "arcanedev/log-viewer": "^11.0",
        "darkaonline/l5-swagger": "^8.6",
        "zircote/swagger-php": "^3.2",
        "mbezhanov/faker-provider-collection": "^2.0.1",
        "mll-lab/laravel-graphql-playground": "^2.5",
        "webonyx/graphql-php": "^14.11",
        "symfony/var-exporter": "^6.0"
    },
        "require-dev": {
        "fakerphp/faker": "^1.21",
        "phpunit/phpunit": "^11.0.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan": "^1.11",
        "laravel/sail": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Uasoft\\Badaso\\Module\\Commerce\\": "packages/badaso/commerce-module/src/",
            "Uasoft\\Badaso\\": "packages/badaso/core/src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
